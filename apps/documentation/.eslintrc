{
  "extends": ["@elixir-cloud"],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "prettier/prettier": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "": "never",
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ]
  }
}
