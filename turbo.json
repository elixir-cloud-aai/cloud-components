{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["topo"]
    },
    "lint": {},
    "lint:fix": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["topo"]
    }
  }
}
