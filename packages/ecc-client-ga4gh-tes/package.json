{
  "name": "ecc-client-ga4gh-tes",
  "description": "Web components for TES",
  "main": "./dist/src/index.js",
  "type": "module",
  "version": "0.1.0",
  "author": "elixir-cloud-aai",
  "license": "MIT",
  "types": "dist/src/index.d.ts",
  "repository": "https://github.com/elixir-cloud-aai/cloud-components",
  "bugs": {
    "url": "https://github.com/elixir-cloud-aai/cloud-components/issues"
  },
  "exports": {
    ".": "./dist/src/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "images",
    "dist"
  ],
  "keywords": [
    "TES",
    "Task Execution Service",
    "Elixir",
    "Cloud Component",
    "Web Component"
  ],
  "scripts": {
    "dev": "concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds\"",
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore",
    "lint:fix": "npm run lint --fix",
    "test": "tsc && wtr --coverage",
    "test:watch": "concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wtr --watch\"",
    "storybook": "tsc && npm run analyze && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds -c .storybook/server.mjs\"",
    "storybook:build": "tsc && npm run analyze && build-storybook",
    "build": "tsc",
    "start:build": "web-dev-server --root-dir dist --app-index index.html --open",
    "analyze": "cem analyze --fast --exclude dist",
    "release": "npm version && npm run build && build publish"
  },
  "dependencies": {
    "@microsoft/fast-components": "^2.30.6",
    "@microsoft/fast-element": "^1.12.0",
    "@microsoft/fast-foundation": "^2.49.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@custom-elements-manifest/analyzer": "^0.4.17",
    "@open-wc/eslint-config": "^9.2.1",
    "@open-wc/testing": "^3.1.6",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@web/dev-server": "^0.1.34",
    "@web/dev-server-storybook": "^0.5.4",
    "@web/test-runner": "^0.14.0",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "concurrently": "^5.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "rimraf": "^3.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5"
  },
  "customElements": "custom-elements.json"
}
