{
  "extends": [
    "@open-wc",
    "elixir"],
  "rules": {
    "import/no-unresolved":"off",
    // "import/extensions":"off"
    "no-unused-expressions":"off",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "error"
    ],
    "import/extensions": [
      "error",
      "always",
      {
        "ignorePackages": true
      }
    ]
  },
  "ignorePatterns": [
    "node_modules/",
    "dist/",
    "*.html"
  ]
}
