{
  "extends": [
    "elixir",
    "plugin:import/typescript",
    "prettier",
    "turbo",
    "standard",
    "plugin:@typescript-eslint/recommended"
  ],
  "plugins": ["import", "prettier"],
  "rules": {
    "no-unused-vars": "off"
  },
  "parserOptions": {
    "project": ["./tsconfig.json"]
  }
}
